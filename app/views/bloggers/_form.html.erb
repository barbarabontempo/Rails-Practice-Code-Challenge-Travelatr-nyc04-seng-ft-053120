<% if @errors %>
    <% @errors.each do |err|%>
        <%= err %>
    <%end%>
<%end%>

<%= form_with model: @blogger do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :bio %>
    <%= f.text_field :bio %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.submit %>
<% end %>